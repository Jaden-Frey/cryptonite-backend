{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/auth/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/login",
        "dest": "index.js"
      },
      {
        "src": "/register",
        "dest": "index.js"
      },
      {
        "src": "/reset-password",
        "dest": "index.js"
      },
      {
        "src": "/logout",
        "dest": "index.js"
      },
      {
        "src": "/favourites",
        "dest": "index.js"
      },
      {
        "src": "/marksum",
        "dest": "index.js"
      },
      {
        "src": "/coins",
        "dest": "index.js"
      },
      {
        "src": "/faq",
        "dest": "index.js"
      },
      {
        "src": "/setup",
        "dest": "index.js"
      },
      {
        "src": "/static/(.*)",
        "dest": "/public/static/$1"
      },
      {
        "src": "/(.*)\\.(?:jpg|jpeg|gif|png|svg|ico|css|js)$",
        "dest": "/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }