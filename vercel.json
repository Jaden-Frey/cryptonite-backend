{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/auth/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/favourites",
        "dest": "index.js"
      },
      {
        "src": "/login",
        "dest": "index.js"
      },
      {
        "src": "/register",
        "dest": "index.js"
      },
      {
        "src": "/reset-password",
        "dest": "index.js"
      },
      {
        "src": "/logout",
        "dest": "index.js"
      },
      {
        "src": "/setup",
        "dest": "index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }